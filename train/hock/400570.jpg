import os

# Function to rename multiple files
def main():
    i = 0
    path = '/home/temp/Documents/CESAR-School/05-Modelos_Preditivos/05-Redes_Neurais/projeto-final/hockey'
    for filename in os.listdir(path):
        my_dest = "400" + str(i) + ".jpg"
        my_source = filename
        my_dest =  my_dest
        # rename() function will rename all the files
        os.rename(my_source, my_dest)
        i += 1


if __name__ == "__main__":
    main()
